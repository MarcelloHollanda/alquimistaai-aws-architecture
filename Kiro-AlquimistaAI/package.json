{
  "name": "fibonacci-aws-setup",
  "version": "1.0.0",
  "description": "Ecossistema Alquimista.AI - Arquitetura Fractal Serverless",
  "main": "bin/app.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:unit:watch": "vitest tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:integration:watch": "vitest tests/integration",
    "test:coverage": "vitest run --coverage",
    "test:security": "vitest run tests/security --reporter=verbose",
    "test:security:watch": "vitest tests/security",
    "test:security:report": "vitest run tests/security --reporter=html --outputFile=./tests/security/reports/test-report.html",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "cdk": "cdk",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "pre-deploy": "npm run build && npm run synth",
    "deploy": "cdk deploy --all",
    "deploy:dev": "cdk deploy --all --context env=dev",
    "deploy:staging": "cdk deploy --all --context env=staging",
    "deploy:prod": "cdk deploy --all --context env=prod",
    "destroy": "cdk destroy --all",
    "bootstrap": "cdk bootstrap",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "alarms:configure": "node -e \"console.log('Use: npm run alarms:configure:dev ou alarms:configure:staging ou alarms:configure:prod')\"",
    "alarms:configure:dev": "bash scripts/configure-alarm-notifications.sh dev",
    "alarms:configure:staging": "bash scripts/configure-alarm-notifications.sh staging",
    "alarms:configure:prod": "bash scripts/configure-alarm-notifications.sh prod",
    "alarms:list": "aws cloudwatch describe-alarms --alarm-name-prefix fibonacci- --query 'MetricAlarms[*].[AlarmName,StateValue]' --output table",
    "alarms:test": "node -e \"console.log('Para testar alarmes, use os comandos no Docs/Deploy/ALARM-QUICK-REFERENCE.md')\"",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "audit:critical": "npm audit --audit-level=critical",
    "security:scan": "npm run audit:critical && npm run lint",
    "security:check": "node scripts/security-check.js",
    "cdk:nag": "cdk synth --context enableNag=true",
    "security:full": "npm run security:check && npm run cdk:nag",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:first": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "release:dry": "standard-version --dry-run",
    "prepare": "husky install",
    "commitlint": "commitlint --edit",
    "commit": "node scripts/commit-helper.js",
    "blue-green-deploy": "ts-node scripts/blue-green-deploy.ts",
    "deploy:blue-green": "npm run build && npm run blue-green-deploy",
    "stack:version": "ts-node scripts/stack-versioning.ts",
    "stack:version:create": "npm run stack:version create",
    "stack:version:list": "npm run stack:version list",
    "stack:version:rollback": "npm run stack:version rollback",
    "stack:version:cleanup": "npm run stack:version cleanup",
    "validate:final": "ts-node scripts/final-deploy-validation.ts",
    "deploy:prod:validate": "npm run validate:final && npm run deploy:prod",
    "document:outputs": "ts-node scripts/document-outputs.ts",
    "document:outputs:prod": "npm run document:outputs prod",
    "deploy:prod:complete": "npm run deploy:prod:validate && npm run document:outputs:prod",
    "generate:inventory": "ts-node scripts/generate-system-inventory.ts",
    "generate:inventory:main": "ts-node scripts/generate-system-inventory.ts --main-only",
    "generate:inventory:index": "ts-node scripts/generate-system-inventory.ts --index-only",
    "validate:inventory": "ts-node scripts/generate-system-inventory.ts --validate-only"
  },
  "keywords": [
    "aws",
    "cdk",
    "serverless",
    "fractal",
    "alquimista",
    "fibonacci",
    "nigredo"
  ],
  "author": "Alquimista.AI",
  "license": "MIT",
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.934.0",
    "@aws-sdk/lib-dynamodb": "^3.934.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@playwright/test": "^1.56.1",
    "@types/aws-lambda": "^8.10.157",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.9",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitest/coverage-v8": "^1.0.0",
    "aws-cdk": "^2.152.0",
    "cdk-nag": "^2.37.55",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-commits-parser": "^6.0.0",
    "conventional-recommended-bump": "^10.0.0",
    "eslint": "^8.57.0",
    "fast-check": "^3.15.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.6",
    "js-yaml": "^4.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.28.1",
    "@aws-lambda-powertools/metrics": "^2.28.1",
    "@aws-lambda-powertools/tracer": "^2.28.1",
    "@aws-sdk/client-eventbridge": "^3.929.0",
    "@aws-sdk/client-rds-data": "^3.609.0",
    "@aws-sdk/client-secrets-manager": "^3.609.0",
    "@aws-sdk/client-sqs": "^3.609.0",
    "aws-cdk-lib": "^2.152.0",
    "aws-sdk": "^2.1577.0",
    "constructs": "^10.3.0",
    "esbuild": "^0.21.5",
    "ioredis": "^5.4.1",
    "pg": "^8.12.0",
    "source-map-support": "^0.5.21",
    "stripe": "^14.21.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  }
}
