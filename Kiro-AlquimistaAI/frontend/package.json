    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.312.0",
    "next": "^14.2.33",
    "next-intl": "^3.19.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.66.0",
    "react-icons": "^5.5.0",
