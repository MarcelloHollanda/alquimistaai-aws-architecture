# ⚠️ VARIÁVEIS DE AMBIENTE PARA TESTES E2E
# IMPORTANTE: Este arquivo é usado APENAS para testes Playwright
# NUNCA usar em produção!

# Bypass de autenticação para testes E2E
E2E_BYPASS_AUTH=true
NEXT_PUBLIC_E2E_BYPASS_AUTH=true

# Ambiente
NODE_ENV=development

# API URLs (mock para testes)
NEXT_PUBLIC_API_URL=http://localhost:3000/api
NEXT_PUBLIC_AWS_REGION=us-east-1

# Cognito (mock para testes)
NEXT_PUBLIC_COGNITO_USER_POOL_ID=mock-pool-id
NEXT_PUBLIC_COGNITO_CLIENT_ID=mock-client-id
NEXT_PUBLIC_COGNITO_DOMAIN=mock-domain

# Desabilitar telemetria
NEXT_TELEMETRY_DISABLED=1
